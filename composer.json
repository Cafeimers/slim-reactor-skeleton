{
    "name": "crazygoat/slim-reactor-skeleton",
    "description": "A Slim Framework skeleton powered by react http server",
    "keywords": ["microframework", "rest", "router", "psr7", "react"],
    "homepage": "https://github.com/crazy-goat/React-Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr HaÅ‚as",
            "email": "dev@crazy-goat.com",
            "homepage": "https://crazy-goat.com/"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "react/http": "^0.8.1",
        "crazygoat/slim-reactor": "^0.2.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "src/app.php 0.0.0.0:8881",
        "goatherd": "goatherd config/goatherd-tcp.json",
        "test": "phpunit"
    }

}
